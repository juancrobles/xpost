loadgraphics

/randgray {
	rand 256 mod 255 div setgray
} def

/randwidth {
	rand 10 mod setlinewidth
} def

/randpoint {
    rand 400 mod
    rand 300 mod
} def

/randline {
    randpoint moveto
    randpoint lineto
} def

/randbox {
    rand 200 mod
    rand 150 mod
    randpoint moveto
    1 index 0 rlineto
    0 exch rlineto
    neg 0 rlineto
    closepath
} def

/randcurve {
    randpoint
    moveto
    randpoint
    randpoint
    randpoint
    curveto
} def

%realtime srand


0 setlinewidth
10 { randbox randgray stroke newpath } repeat
showpage


0 setlinewidth
10 { randline randgray stroke } repeat
showpage

10 { randline randwidth randgray stroke } repeat
showpage

10 { randbox randgray fill newpath } repeat
showpage

0 setlinewidth
10 { randcurve randgray stroke } repeat
showpage

6 setlinewidth
10 { randcurve randwidth randgray stroke } repeat
showpage
